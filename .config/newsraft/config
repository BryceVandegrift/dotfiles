set clean-database-on-startup true
set menu-item-sorting unread-desc
set download-timeout 20
set reload-period 30

# Colors
set color-list-feed-unread bold yellow
set color-list-section-unread bold yellow

# Custom keybinds
bind U mark-unread
bind L exec lynx "%l"
bind m exec mpv "%l"
bind x exec curl -fLs "%l" | imgpipe
bind z exec curl -fLs "%l" | zathura -
